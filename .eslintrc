{
  "extends": "airbnb/base",
  "ecmaFeatures": {
    "templateStrings": true
  },
  "rules": {
    "comma-dangle": [
      "error",
      "only-multiline"
    ],
    "no-eq-null": 0,
    "no-console": 0,
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "always",
        "named": "never"
      }
    ],
    "func-names": 0,
    "id-length": 0,
    "arrow-body-style": 0,
    "max-len": [
      "error",
      120,
      2,
      {
        "ignoreUrls": true,
        "ignoreTrailingComments": true,
        "ignoreComments": true
      }
    ]
  }
}
